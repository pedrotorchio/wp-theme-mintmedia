    "@vue/eslint-config-prettier": "^4.0.1",
    "assets-webpack-plugin": "^3.9.7",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.11.0",
    "pug": "^2.0.3",
